{
    "manifest_version": 3,
    "name": "Hue Tab",
    "description": "A customizable HSL color picker on every new tab page",
    "version": "1.0.0",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "offline_enabled": true,
    "permissions": [
        "storage"
    ],
    "options_page": "options.html",
    "incognito": "split"
}